{
  "buildCommand": "pnpm build",
  "installCommand": "corepack enable && corepack prepare pnpm@10.15.0 --activate && pnpm install",
  "framework": "nextjs",
  "version": 3,
  "builds": [
    {
      "src": "packages/leenders-coaching-nl/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/packages/leenders-coaching-nl/$1"
    }
  ]
}
