{
  "name": "studio-leenders-coaching-nl",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build && sanity schema deploy --manifest-dir ./dist/static",
    "deploy": "sanity deploy",
    "extract-schema": "sanity schema extract",
    "generate-types": "sanity typegen generate",
    "update-types": "pnpm run extract-schema && pnpm run generate-types",
    "validate:schema": "sanity schema extract --enforce-required-fields",
    "verify:build": "test -d dist && test -f dist/index.html && echo 'âœ… Studio build verified'",
    "package:build": "tar -czf studio-build.tar.gz -C . dist 2>/dev/null || echo 'No build artifacts to package'"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/client": "7.14.1",
    "@sanity/presentation": "2.0.0",
    "@sanity/types": "5.9.0",
    "@sanity/vision": "5.9.0",
    "dotenv": "17.2.4",
    "lodash.defaults": "4.2.0",
    "lodash.difference": "4.5.0",
    "lodash.flatten": "4.4.0",
    "lodash.union": "4.6.0",
    "nanoid": "5.1.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-is": "19.2.4",
    "sanity": "5.9.0",
    "styled-components": "6.3.9"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "6.0.0",
    "@types/node": "25.0.10",
    "@types/react": "19.2.10",
    "eslint": "9.39.2",
    "prettier": "3.8.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.29.2",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": "10.29.2"
  }
}
