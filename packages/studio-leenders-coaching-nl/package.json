{
  "name": "studio-leenders-coaching-nl",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "extract-schema": "sanity schema extract",
    "generate-types": "sanity typegen generate",
    "update-types": "pnpm run extract-schema && pnpm run generate-types",
    "validate:schema": "sanity schema extract --enforce-required-fields",
    "verify:build": "test -d dist && test -f dist/index.html && echo 'âœ… Studio build verified'",
    "package:build": "tar -czf studio-build.tar.gz -C . dist 2>/dev/null || echo 'No build artifacts to package'",
    "seed:create-home-page": "node --loader ts-node/esm scripts/content-seeding/create-home-page.ts",
    "seed:create-home-page:dry-run": "node --loader ts-node/esm scripts/content-seeding/create-home-page.ts --dry-run",
    "seed:create-about-page": "node --loader ts-node/esm scripts/content-seeding/create-about-page.ts",
    "seed:create-about-page:dry-run": "node --loader ts-node/esm scripts/content-seeding/create-about-page.ts --dry-run",
    "seed:create-coaching-page": "node --loader ts-node/esm scripts/content-seeding/create-coaching-page.ts",
    "seed:create-coaching-page:dry-run": "node --loader ts-node/esm scripts/content-seeding/create-coaching-page.ts --dry-run",
    "seed:create-approach-page": "node --loader ts-node/esm scripts/content-seeding/create-approach-page.ts",
    "seed:create-approach-page:dry-run": "node --loader ts-node/esm scripts/content-seeding/create-approach-page.ts --dry-run",
    "seed:test-home": "node --loader ts-node/esm scripts/content-seeding/test-homepage.ts",
    "seed:create-contact-page": "node --loader ts-node/esm scripts/content-seeding/create-contact-page.ts",
    "seed:create-contact-page:dry-run": "node --loader ts-node/esm scripts/content-seeding/create-contact-page.ts --dry-run",
    "seed:create-blog-page": "node --loader ts-node/esm scripts/content-seeding/create-blog-page.ts",
    "seed:create-blog-page:dry-run": "node --loader ts-node/esm scripts/content-seeding/create-blog-page.ts --dry-run",
    "seed:all": "node --loader ts-node/esm scripts/content-seeding/seed-all-pages.ts",
    "seed:all:dry-run": "node --loader ts-node/esm scripts/content-seeding/seed-all-pages.ts --dry-run",
    "seed:test": "node --loader ts-node/esm scripts/content-seeding/test-script.ts",
    "seed:test:dry-run": "node --loader ts-node/esm scripts/content-seeding/test-script.ts --dry-run",
    "dataset:list": "node --loader ts-node/esm scripts/dataset-aliases.ts list",
    "dataset:create": "node --loader ts-node/esm scripts/dataset-aliases.ts create",
    "dataset:update": "node --loader ts-node/esm scripts/dataset-aliases.ts update",
    "dataset:delete": "node --loader ts-node/esm scripts/dataset-aliases.ts delete",
    "dataset:clone": "node --loader ts-node/esm scripts/dataset-aliases.ts clone",
    "dataset:deploy": "node --loader ts-node/esm scripts/dataset-aliases.ts deploy"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/client": "7.11.0",
    "@sanity/presentation": "2.0.0",
    "@sanity/types": "4.6.1",
    "@sanity/vision": "4.6.1",
    "dotenv": "17.2.2",
    "lodash.defaults": "4.2.0",
    "lodash.difference": "4.5.0",
    "lodash.flatten": "4.4.0",
    "lodash.union": "4.6.0",
    "nanoid": "5.1.5",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-is": "19.1.1",
    "sanity": "4.6.1",
    "styled-components": "6.1.19"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "5.0.2",
    "@types/node": "24.3.1",
    "@types/react": "19.1.12",
    "eslint": "9.34.0",
    "prettier": "3.6.2",
    "ts-node": "10.9.2",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": "10.15.1"
  }
}
