{
  "buildScriptName": "build-storybook",
  "exitZeroOnChanges": true,
  "ignoreLastBuildOnBranch": "main",
  "onlyChanged": true,
  "storybookBaseDir": "packages/leenders-coaching-nl",
  "storybookConfigDir": "packages/leenders-coaching-nl/.storybook",
  "externals": [
    "packages/leenders-coaching-nl/public/**",
    "packages/leenders-coaching-nl/src/assets/**"
  ],
  "untraced": [
    "package.json",
    "pnpm-lock.yaml",
    "*.md",
    "*.log",
    "coverage/**",
    ".next/**",
    "node_modules/**",
    "storybook-static/**"
  ],
  "skip": false,
  "storybookBuildDir": "storybook-static",
  "storybookUrl": null,
  "uploadTimeout": 300000
}
