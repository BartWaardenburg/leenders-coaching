{
  "name": "leenders-coaching-nl",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "update-types": "cd ../studio-leenders-coaching-nl && pnpm run update-types"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "pnpm type-check"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "tabWidth": 2
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.1",
    "@portabletext/react": "4.0.2",
    "@react-email/components": "0.5.1",
    "@react-email/render": "1.2.1",
    "@sanity/image-url": "1.1.0",
    "@vercel/analytics": "1.5.0",
    "motion": "12.23.12",
    "next": "15.5.2",
    "next-sanity": "9.12.3",
    "next-themes": "0.4.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "react-icons": "5.5.0",
    "resend": "4.8.0",
    "tailwind-merge": "3.3.1",
    "zod": "4.1.4"
  },
  "devDependencies": {
    "@eslint/js": "9.34.0",
    "@next/bundle-analyzer": "15.5.2",
    "@next/env": "15.5.2",
    "@next/eslint-plugin-next": "15.5.2",
    "@portabletext/types": "2.0.15",
    "@storybook/addon-a11y": "9.1.3",
    "@storybook/addon-links": "9.1.3",
    "@storybook/addon-themes": "9.1.3",
    "@storybook/nextjs": "9.1.3",
    "@storybook/react": "9.1.3",
    "@tailwindcss/postcss": "4.1.12",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "22.18.0",
    "autoprefixer": "10.4.21",
    "css-loader": "7.1.2",
    "dotenv": "17.2.1",
    "eslint-config-next": "15.5.2",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "9.1.3",
    "postcss": "8.5.6",
    "postcss-loader": "8.1.1",
    "prettier-plugin-tailwindcss": "0.6.14",
    "storybook": "9.1.3",
    "style-loader": "4.0.0",
    "tailwindcss": "4.1.12",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": "10.15.0"
  },
  "packageManager": "pnpm@10.15.0"
}
