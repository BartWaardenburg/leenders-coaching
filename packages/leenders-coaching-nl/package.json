{
  "name": "leenders-coaching-nl",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "verify:build": "test -d .next && test -f .next/BUILD_ID && echo 'âœ… Frontend build verified'",
    "package:build": "tar -czf frontend-build.tar.gz -C . .next 2>/dev/null || echo 'No build artifacts to package'",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "lint": "eslint .",
    "lint:format": "eslint . --format json",
    "type-check": "tsc --noEmit",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "test:coverage": "vitest run --config vitest.config.ts  --coverage",
    "test:coverage:ci": "vitest run --config vitest.ci.config.ts --coverage --reporter=verbose",
    "test:ui": "vitest --ui --config vitest.config.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --stats-json",
    "build-storybook:analyze": "cross-env ANALYZE_STORYBOOK=true storybook build",
    "chromatic": "chromatic",
    "chromatic:changed": "chromatic --only-changed",
    "analyze:storybook": "pnpm dlx bundlesize2 check --config bundlesize.config.json",
    "analyze:storybook:vite": "node scripts/collect-vite-bundle-metrics.cjs",
    "analyze:bundle": "cross-env ANALYZE=true ANALYZE_JSON=true next build",
    "verify:performance": "pnpm run build && size-limit",
    "ci:test-a11y": "pnpm dlx start-server-and-test 'pnpm dlx http-server storybook-static -p 6006' http://localhost:6006 'pnpm dlx @storybook/test-runner --url http://localhost:6006'",
    "validate:tailwind": "postcss src/app/globals.css -o /tmp/test.css --config postcss.config.mjs && echo 'âœ… Tailwind CSS configuration is valid' && rm -f /tmp/test.css",
    "update-types": "cd ../studio-leenders-coaching-nl && pnpm run update-types"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "browserslist": {
    "production": [
      "chrome >= 90",
      "edge >= 90",
      "firefox >= 88",
      "safari >= 14",
      "opera >= 76"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
