{
  "name": "leenders-coaching-nl",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "verify:build": "test -d .next && test -f .next/BUILD_ID && echo 'âœ… Frontend build verified'",
    "package:build": "tar -czf frontend-build.tar.gz -C . .next 2>/dev/null || echo 'No build artifacts to package'",
    "clean": "rm -rf .next && rm -rf node_modules/.cache",
    "lint": "eslint .",
    "lint:format": "eslint . --format json",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage --allowOnly=false",
    "test:coverage:ci": "vitest run --config vitest.ci.config.ts --coverage --reporter=verbose --allowOnly=false",
    "test:ui": "vitest --ui",
    "test-storybook": "vitest run --config vitest.storybook.config.ts --allowOnly=false",
    "test-storybook:coverage": "vitest run --config vitest.storybook.config.ts --coverage --allowOnly=false",
    "test:all": "vitest run --config vitest.workspace.ts --coverage --allowOnly=false",
    "test:all:coverage": "vitest run --config vitest.workspace.ts --coverage --allowOnly=false",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --stats-json",
    "build-storybook:analyze": "cross-env ANALYZE_STORYBOOK=true storybook build",
    "chromatic": "chromatic",
    "chromatic:changed": "chromatic --only-changed",
    "analyze:storybook": "pnpm dlx bundlesize2 check --config bundlesize.config.json",
    "analyze:storybook:vite": "node scripts/collect-vite-bundle-metrics.cjs",
    "analyze:bundle": "cross-env ANALYZE=true ANALYZE_JSON=true next build",
    "verify:performance": "pnpm run build && size-limit",
    "ci:install-playwright": "pnpm dlx playwright install --with-deps chromium",
    "ci:test-a11y": "pnpm dlx start-server-and-test 'pnpm dlx http-server storybook-static -p 6006' http://localhost:6006 'pnpm dlx @storybook/test-runner --url http://localhost:6006'",
    "validate:tailwind": "postcss src/app/globals.css -o /tmp/test.css --config postcss.config.mjs && echo 'âœ… Tailwind CSS configuration is valid' && rm -f /tmp/test.css",
    "update-types": "cd ../studio-leenders-coaching-nl && pnpm run update-types"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.1",
    "@portabletext/react": "4.0.3",
    "@react-email/components": "0.5.2",
    "@react-email/render": "1.2.2",
    "@sanity/image-url": "1.2.0",
    "@sanity/visual-editing": "3.0.4",
    "@vercel/analytics": "1.5.0",
    "@vercel/related-projects": "1.0.0",
    "motion": "12.23.12",
    "next": "15.5.2",
    "next-sanity": "10.0.16",
    "next-themes": "0.4.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "react-icons": "5.5.0",
    "resend": "6.0.2",
    "tailwind-merge": "3.3.1",
    "zod": "4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "9.35.0",
    "@lhci/cli": "0.15.1",
    "@next/bundle-analyzer": "15.5.2",
    "@next/env": "15.5.2",
    "@next/eslint-plugin-next": "15.5.2",
    "@portabletext/types": "2.0.15",
    "@size-limit/file": "11.2.0",
    "@size-limit/preset-app": "11.2.0",
    "@size-limit/webpack": "11.2.0",
    "@storybook/addon-a11y": "9.1.5",
    "@storybook/addon-actions": "9.0.8",
    "@storybook/addon-docs": "9.1.5",
    "@storybook/addon-links": "9.1.5",
    "@storybook/addon-themes": "9.1.5",
    "@storybook/addon-vitest": "9.1.5",
    "@storybook/nextjs-vite": "9.1.5",
    "@storybook/react": "9.1.5",
    "@tailwindcss/postcss": "4.1.13",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "24.3.1",
    "@types/webpack-stats-plugin": "0.3.5",
    "@vitest/browser": "3.2.4",
